  <script>
    document.addEventListener("DOMContentLoaded", function () {
      const gallery1Images = document.querySelectorAll(
        "#gallery1 img:not(.no-animate)"
      );
      const gallery2Images = document.querySelectorAll(
        "#gallery2 img:not(.no-animate)"
      );
      // const scrollThresholds = [
      //   0, 400, 700, 900, 1200, 1900, 2300, 2800, 3000,
      // ];

      function animateImages(images) {
        images.forEach((img) => {
          img.style.animation = "sanjay 2s forwards";
        });
      }

      function getScrollThresholds() {
        const screenWidth = window.innerWidth;
        if (screenWidth < 600) {
          // Adjust thresholds for smaller screens
          return [0, 100, 200, 300, 400, 500, 600, 700, 800];
        }
        else if (screenWidth > 600 && screenWidth < 1500) {
          return [0, 400, 600, 700, 950, 1000, 1150, 1260, 1300];
        }
        else {
          // Default thresholds for larger screens
          return [430, 800, 1200, 1800, 2100, 2400, 2800, 3200, 3800];
        }
      }

      window.addEventListener("scroll", function () {
        const currentScrollPos = window.scrollY;
        console.log(currentScrollPos);
        const scrollThresholds = getScrollThresholds();
        for (let i = 0; i < scrollThresholds.length; i++) {
          if (currentScrollPos >= scrollThresholds[i]) {
            animateImages([gallery1Images[i], gallery2Images[i]]);
          }
        }
      });
    });
  </script>

